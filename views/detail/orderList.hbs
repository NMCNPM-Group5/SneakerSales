<link rel="stylesheet" href="../../public/css/orderList.css">
<div class="container-fluid">
    <table>
        <thead>
            <tr>
                <th>Mã đơn hàng</th>
                <th>Ngày mua</th>
                <th>Sản phẩm</th>
                <th>Tổng tiền</th>
                <th>Trạng thái đơn hàng</th>
            </tr>
        </thead>
        <tbody>
            {{#each order}}
            <tr>
                <td><a id="{{id}}" href="">{{id}}</a></td>
                <td>{{createdAt}}</td>
                <td>{{product}}</td>
                <td>{{price}}</td>
                <td>{{status}}</td>
            </tr>
            <script>
                var temp = "#" + "{{id}}";
                $(temp).attr('href', '/order/' + '{{id}}');
            </script>
            {{/each}}
        </tbody>
    </table>


</div>